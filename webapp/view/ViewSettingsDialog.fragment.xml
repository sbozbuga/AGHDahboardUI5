<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<ViewSettingsDialog
		confirm="onConfirmViewSettings">
		<sortItems>
			<ViewSettingsItem text="Time" key="time" selected="true" />
			<ViewSettingsItem text="Client" key="client" />
			<ViewSettingsItem text="Status" key="status" />
			<ViewSettingsItem text="Elapsed" key="elapsedMs" />
		</sortItems>
		<groupItems>
			<ViewSettingsItem text="Client" key="client" />
			<ViewSettingsItem text="Status" key="status" />
			<ViewSettingsItem text="Question Type" key="question/type" />
		</groupItems>
		<filterItems>
			<ViewSettingsFilterItem
				text="Status"
				key="status"
				multiSelect="true">
				<items>
					<ViewSettingsItem text="OK" key="OK" />
					<ViewSettingsItem text="Blocked" key="Blocked" />
				</items>
			</ViewSettingsFilterItem>
		</filterItems>
	</ViewSettingsDialog>
</core:FragmentDefinition>
